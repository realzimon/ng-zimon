{
  "name": "ng-zimon",
  "version": "1.0.0",
  "description": "Angular2 frontend for Zimon",
  "scripts": {
    "start": "tslint ./app/**/*.ts -t verbose && concurrently \"tsc -w\" \"lite-server\"",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "lite": "lite-server",
    "prod-server": "tslint ./app/**/*.ts -t verbose && serve --port 3000 .",
    "test": "tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "test-once": "tsc && karma start karma.conf.js --single-run",
    "tsc": "tsc",
    "tsc:w": "tsc -w"
  },
